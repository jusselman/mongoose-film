<!DOCTYPE html>
<html>
  <head>
    <title>Film</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Release<br>Year</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% films.forEach(function(f) { %>
        <tr>
          <td><%= f.title %></td>
          <td><%= f.releaseYear %></td>
          <td><a href="/films/<%= f._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

</body>
</html>